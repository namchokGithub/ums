<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <title>@ViewData["Title"] - User Management System</title>
    <link rel="shortcut icon" href="https://image.flaticon.com/icons/svg/3266/3266632.svg" />

    <partial name="AdminLTE/_Styles" />
</head>
<body>

    <!-- content -->
    <div class="content">
        <div class="container-fluid">
            @RenderBody()
        </div>
    </div>
    <!-- ./content -->
    <partial name="AdminLTE/_Scripts" />
    @RenderSection("Scripts", required: false)

</body>
</html>